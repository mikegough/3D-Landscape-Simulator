<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <module name="stsim-stock-flow" displayName="Stocks and Flows" version="1.0024" configVersion="1.0000">
    <transformers>
      <transformer name="stock-flow-core-transformer" displayName="Stocks and Flows" className="SyncroSim.STSimStockFlow.StockFlowTransformer" classAssembly="SyncroSim.STSimStockFlow">
        <dataFeeds>
          <dataFeed name="stock-flow-stock-group-datafeed" displayName="Stock Groups" dataScope="Project">
            <dataSheets>
              <dataSheet name="SF_StockGroup" displayName="Stock Groups" valueMember="StockGroupID" displayMember="Name">
                <columns>
                  <column name="StockGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                  <column name="Units" dataType="String" isOptional="True"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-stock-type-datafeed" displayName="Stock Types" dataScope="Project">
            <dataSheets>
              <dataSheet name="SF_StockType" displayName="Stock Types" valueMember="StockTypeID" displayMember="Name">
                <columns>
                  <column name="StockTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-flow-group-datafeed" displayName="Flow Groups" dataScope="Project">
            <dataSheets>
              <dataSheet name="SF_FlowGroup" displayName="Flow Groups" valueMember="FlowGroupID" displayMember="Name">
                <columns>
                  <column name="FlowGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                  <column name="Units" dataType="String" isOptional="True"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-flow-type-datafeed" displayName="Flow Types" dataScope="Project">
            <dataSheets>
              <dataSheet name="SF_FlowType" displayName="Flow Types" valueMember="FlowTypeID" displayMember="Name">
                <columns>
                  <column name="FlowTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-terminology-datafeed" displayName="Terminology" dataScope="Project">
            <dataSheets>
              <dataSheet name="SF_Terminology" displayName="Terminology" isSingleRow="True">
                <columns>
                  <column name="TerminologyID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="StockUnits" dataType="String"/>
                </columns>
                <defaultRecords>
                  <record columns="StockUnits" values="tons C"/>
                </defaultRecords>
              </dataSheet>
            </dataSheets>
          </dataFeed>        
          <dataFeed name="stock-flow-stock-limit-datafeed" displayName="Stock Limits" dataScope="Scenario">
            <dataSheets>
              <dataSheet name="SF_StockLimit" displayName="Stock Limits">
                <columns>
                  <column name="StockLimitID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="Timestep" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="TimestepUnits" format="d"/>
                  <column name="StockTypeID" dataType="Integer" displayName="Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="StratumID" dataType="Integer" displayName="Primary Stratum" validationType="DataSheet" formula1="STSim_Stratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="PrimaryStratumLabel"/>
                  <column name="SecondaryStratumID" dataType="Integer" displayName="Secondary Stratum" validationType="DataSheet" formula1="STSim_SecondaryStratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="SecondaryStratumLabel"/>
                  <column name="StateClassID" dataType="Integer" displayName="State Class" validationType="DataSheet" formula1="STSim_StateClass" isOptional="True"/>
                  <column name="StockMinimum" dataType="Double" displayName="Stock Min" validationType="Decimal"/>
                  <column name="StockMaximum" dataType="Double" displayName="Stock Max" validationType="Decimal"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="StockMinimum|StockMaximum"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>      
          <dataFeed name="stock-flow-flow-pathway-datafeed" displayName="Flow Pathways" dataScope="Scenario" viewClassName="SyncroSim.STSimStockFlow.FlowPathwayDataFeedView" viewClassAssembly="SyncroSim.STSimStockFlow">
            <dataSheets>
              <dataSheet name="SF_FlowPathwayDiagram" displayName="Flow Pathway Diagram" className="SyncroSim.STSimStockFlow.FlowPathwayDiagramDataSheet" classAssembly="SyncroSim.STSimStockFlow">
                <columns>
                  <column name="FlowPathwayDiagramID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="StockTypeID" dataType="Integer" displayName="Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="Location" dataType="String" columnAlignment="Center" allowDbNull="False"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="StockTypeID"/>
                  <validation validationType="Unique" columns="Location"/>
                </validations>
              </dataSheet>                
              <dataSheet name="SF_FlowPathway" displayName="Flow Pathways" className="SyncroSim.STSimStockFlow.FlowPathwayDataSheet" classAssembly="SyncroSim.STSimStockFlow">
                <columns>
                  <column name="FlowPathwayID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="FromStratumID" dataType="Integer" displayName="From Stratum" validationType="DataSheet" formula1="STSim_Stratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="PrimaryStratumLabel"/>
                  <column name="FromStateClassID" dataType="Integer" displayName="From State Class" validationType="DataSheet" formula1="STSim_StateClass" isOptional="True"/>
                  <column name="FromAgeMin" dataType="Integer" displayName="From Min Age" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="FromStockTypeID" dataType="Integer" displayName="From Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="ToStratumID" dataType="Integer" displayName="To Stratum" validationType="DataSheet" formula1="STSim_Stratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="PrimaryStratumLabel"/>
                  <column name="ToStateClassID" dataType="Integer" displayName="To State Class" validationType="DataSheet" formula1="STSim_StateClass" isOptional="True"/>
                  <column name="ToAgeMin" dataType="Integer" displayName="To Min Age" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="ToStockTypeID" dataType="Integer" displayName="To Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="TransitionGroupID" dataType="Integer" displayName="Transition Group" validationType="DataSheet" formula1="STSim_TransitionGroup" isOptional="True"/>
                  <column name="StateAttributeTypeID" dataType="Integer" displayName="State Attribute Type" validationType="DataSheet" formula1="STSim_StateAttributeType" isOptional="True"/>
                  <column name="FlowTypeID" dataType="Integer" displayName="Flow Type" validationType="DataSheet" formula1="SF_FlowType" allowDbNull="False"/>
                  <column name="Multiplier" displayName="Multiplier" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0" allowDbNull="False"/>
                </columns>
              </dataSheet>                                
            </dataSheets>
          </dataFeed>                    
          <dataFeed name="stock-flow-initial-stocks-datafeed" displayName="Initial Stocks" dataScope="Scenario" viewClassName="SyncroSim.STSimStockFlow.InitialStocksDataFeedView" viewClassAssembly="SyncroSim.STSimStockFlow">
            <dataSheets>
              <dataSheet name="SF_InitialStockNonSpatial" displayName="Initial Stock - Non Spatial">
                <columns>
                  <column name="InitialStockNonSpatialID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="StockTypeID" dataType="Integer" displayName="Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="StateAttributeTypeID" displayName="State Attribute Type" dataType="Integer" validationType="DataSheet" formula1="STSim_StateAttributeType" allowDbNull="False"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="StockTypeID"/>
                </validations>
              </dataSheet>
              <dataSheet name="SF_InitialStockSpatial" displayName="Initial Stock - Spatial">
                <columns>
                  <column name="InitialStockSpatialID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="StockTypeID" dataType="Integer"  displayName="Stock" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False"/>
                  <column name="RasterFileName" displayName="Raster File Name" dataType="String" allowDbNull="False" isExternalFile="True"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="StockTypeID"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-stock-group-membership-datafeed" displayName="Stock Group Membership" dataScope="Scenario">
            <dataSheets>
              <dataSheet name="SF_StockTypeGroupMembership" displayName="Stock Type-Group Membership">
                <columns>
                  <column name="StockTypeGroupMembershipID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="StockTypeID" dataType="Integer" displayName="Stock Type" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="StockGroupID" dataType="Integer" displayName="Stock Group" validationType="DataSheet" formula1="SF_StockGroup" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="Value" dataType="Double" isOptional="True" format="N8"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="StockTypeID|StockGroupID"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-flow-group-membership-datafeed" displayName="Flow Group Membership" dataScope="Scenario">
            <dataSheets>
              <dataSheet name="SF_FlowTypeGroupMembership" displayName="Flow Type-Group Membership">
                <columns>
                  <column name="FlowTypeGroupMembershipID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="FlowTypeID" dataType="Integer" displayName="Flow Type" validationType="DataSheet" formula1="SF_FlowType" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="FlowGroupID" dataType="Integer" displayName="Flow Group" validationType="DataSheet" formula1="SF_FlowGroup" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="Value" dataType="Double" isOptional="True" format="N8"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="FlowTypeID|FlowGroupID"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>            
          <dataFeed name="stock-flow-flow-multiplier-datafeed" displayName="Flow Multipliers" dataScope="Scenario">
            <dataSheets>
              <dataSheet name="SF_FlowMultiplier" displayName="Flow Multipliers">
                <columns>
                  <column name="FlowMultiplierID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="Timestep" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="TimestepUnits" format="d"/>
                  <column name="StratumID" dataType="Integer" displayName="Stratum" validationType="DataSheet" formula1="STSim_Stratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="PrimaryStratumLabel"/>
                  <column name="SecondaryStratumID" dataType="Integer" displayName="Secondary Stratum" validationType="DataSheet" formula1="STSim_SecondaryStratum" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="SecondaryStratumLabel"/>
                  <column name="StateClassID" dataType="Integer" displayName="State Class" validationType="DataSheet" formula1="STSim_StateClass" isOptional="True"/>
                  <column name="FlowGroupID" dataType="Integer" displayName="Flow Group" validationType="DataSheet" formula1="SF_FlowGroup" allowDbNull="False"/>
                  <column name="Value" displayName="Multiplier" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0"/>
                  <column name="DistributionType" displayName="Multiplier Distribution" dataType="Integer" validationType="DataSheet" formula1="Stats_DistributionType" isOptional="True"/>
                  <column name="DistributionSD" displayName="Multiplier SD" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0" isOptional="True"/>
                  <column name="DistributionMin" displayName="Multiplier Min" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0" isOptional="True"/>
                  <column name="DistributionMax" displayName="Multiplier Max" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0" isOptional="True"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="DistributionMin|DistributionMax"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-flow-spatial-multiplier-datafeed" displayName="Spatial Multipliers" dataScope="Scenario" viewClassName="SyncroSim.STSimStockFlow.SpatialMultiplierDataFeedView" viewClassAssembly="SyncroSim.STSimStockFlow">
            <dataSheets>
              <dataSheet name="SF_FlowSpatialMultiplier" displayName="Flow Spatial Multipliers">
                <columns>
                  <column name="FlowSpatialMultiplierID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="Timestep" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True" displayNameSource="STSim_Terminology" displayNameColumn="TimestepUnits" format="d"/>
                  <column name="FlowGroupID" dataType="Integer" displayName="Flow Group" validationType="DataSheet" formula1="SF_FlowGroup" allowDbNull="False"/>
                  <column name="MultiplierFileName" displayName="Multiplier File Name" dataType="String" isExternalFile="True" allowDbNull="False"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="Iteration|Timestep|FlowGroupID"/>
                </validations>
              </dataSheet>
            </dataSheets>
          </dataFeed>                    
          <dataFeed name="stock-flow-output-options-datafeed" displayName="Output Options" dataScope="Scenario" viewClassName="SyncroSim.STSimStockFlow.OutputOptionsDataFeedView" viewClassAssembly="SyncroSim.STSimStockFlow">
            <dataSheets>
              <dataSheet name="SF_OutputOptions" displayName="SF Output Options" isSingleRow="True">
                <columns>
                  <column name="OutputOptionsID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="SummaryOutputST" dataType="Boolean"/>
                  <column name="SummaryOutputSTTimesteps" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="SummaryOutputFL" dataType="Boolean"/>
                  <column name="SummaryOutputFLTimesteps" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="SpatialOutputST" dataType="Boolean"/>
                  <column name="SpatialOutputSTTimesteps" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="SpatialOutputFL" dataType="Boolean"/>
                  <column name="SpatialOutputFLTimesteps" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-output-stock-data-feed" dataScope="Scenario" isOutput="True">
            <dataSheets>
              <dataSheet name="SF_OutputStock" displayName="Stock">
                <columns>
                  <column name="ScenarioID" dataType="Integer" isCompositeIndex="True"/>
                  <column name="Iteration" dataType="Integer" isCompositeIndex="True" format="d"/>
                  <column name="Timestep" dataType="Integer" isCompositeIndex="True" format="d"/>
                  <column name="StratumID" displayName="Stratum" dataType="Integer" validationType="DataSheet" formula1="STSim_Stratum" allowDbNull="False" displayNameSource="STSim_Terminology" displayNameColumn="PrimaryStratumLabel" isCompositeIndex="True"/>
                  <column name="SecondaryStratumID" displayName="Secondary Stratum" dataType="Integer" validationType="DataSheet" formula1="STSim_SecondaryStratum" allowDbNull="True" displayNameSource="STSim_Terminology" displayNameColumn="SecondaryStratumLabel" isCompositeIndex="True"/>
                  <column name="StateClassID" dataType="Integer" displayName="State Class" validationType="DataSheet" formula1="STSim_StateClass" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="StockTypeID" dataType="Integer" displayName="Stock Type" validationType="DataSheet" formula1="SF_StockType" isCompositeIndex="True"/>
                  <column name="Amount" dataType="Double"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="stock-flow-output-flow-data-feed" dataScope="Scenario" isOutput="True">
            <dataSheets>
              <dataSheet name="SF_OutputFlow" displayName="Flow">
                <columns>
                  <column name="ScenarioID" dataType="Integer" isCompositeIndex="True"/>
                  <column name="Iteration" dataType="Integer" isCompositeIndex="True" format="d"/>
                  <column name="Timestep" dataType="Integer" isCompositeIndex="True" format="d"/>
                  <column name="FromStratumID" displayName="From Stratum" dataType="Integer" validationType="DataSheet" formula1="STSim_Stratum" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="FromSecondaryStratumID" displayName="From Secondary Stratum" dataType="Integer" validationType="DataSheet" formula1="STSim_SecondaryStratum" allowDbNull="True" isCompositeIndex="True"/>
                  <column name="FromStateClassID" dataType="Integer" displayName="From State Class" validationType="DataSheet" formula1="STSim_StateClass" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="FromStockTypeID" dataType="Integer" displayName="From Stock Type" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="TransitionTypeID" dataType="Integer" displayName="From Transition Type" validationType="DataSheet" formula1="STSim_TransitionType" allowDbNull="True" isCompositeIndex="True"/>
                  <column name="ToStratumID" displayName="To Stratum" dataType="Integer" validationType="DataSheet" formula1="STSim_Stratum" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="ToStateClassID" dataType="Integer" displayName="To State Class" validationType="DataSheet" formula1="STSim_StateClass" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="ToStockTypeID" dataType="Integer" displayName="To Stock Type" validationType="DataSheet" formula1="SF_StockType" allowDbNull="False" isCompositeIndex="True"/>
                  <column name="FlowTypeID" dataType="Integer" isCompositeIndex="True"/>                 
                  <column name="Amount" dataType="Double"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
        </dataFeeds>
      </transformer>
      <transformer name="stock-flow-summary-stock-report-transformer" displayName="Stocks" className="SyncroSim.STSimStockFlow.SummaryStockReport" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-summary-flow-report-transformer" displayName="Flows" className="SyncroSim.STSimStockFlow.SummaryFlowReport" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-stock-raster-map-transformer" displayName="Stocks" className="SyncroSim.STSimStockFlow.StockRasterMap" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-flow-raster-map-transformer" displayName="Flows" className="SyncroSim.STSimStockFlow.FlowRasterMap" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-stock-group-raster-map-transformer" displayName="Stock Groups" className="SyncroSim.STSimStockFlow.StockGroupRasterMap" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-flow-group-raster-map-transformer" displayName="Flow Groups" className="SyncroSim.STSimStockFlow.FlowGroupRasterMap" classAssembly="SyncroSim.STSimStockFlow">
      </transformer>
      <transformer name="stock-flow-add-on-transformer" displayName="Stocks and Flows" isAddOn="True" extendsModel="stsim-model-transformer" extendsTransformer="stsim-core-transformer" className="SyncroSim.STSimStockFlow.StockFlowAddOn" classAssembly="SyncroSim.STSimStockFlow">
        <modelRunTransformers>
          <transformer name="stock-flow-core-transformer"/>          
        </modelRunTransformers>
        <includeTransformers>
          <transformer name="stock-flow-summary-stock-report-transformer"/>
          <transformer name="stock-flow-summary-flow-report-transformer"/>
          <transformer name="stock-flow-stock-raster-map-transformer"/>
          <transformer name="stock-flow-flow-raster-map-transformer"/>
          <transformer name="stock-flow-stock-group-raster-map-transformer"/>
          <transformer name="stock-flow-flow-group-raster-map-transformer"/>
        </includeTransformers>
        <layouts>
          <layout name ="ProjectDataFeeds" insertAfter="Ages">
            <group name="Stocks and Flows">
              <item name="stock-flow-stock-group-datafeed"/>
              <item name="stock-flow-stock-type-datafeed"/>
              <item name="stock-flow-flow-group-datafeed"/>
              <item name="stock-flow-flow-type-datafeed"/>              
              <item name="stock-flow-terminology-datafeed"/>                             
            </group>
          </layout>
          <layout name="ScenarioDataFeeds" appendTo="Advanced">
            <group name="Stocks and Flows">
              <item name="stock-flow-flow-pathway-datafeed"/>
              <item name="stock-flow-initial-stocks-datafeed"/>              
              <item name="stock-flow-output-options-datafeed"/>
              <item name="stock-flow-stock-group-membership-datafeed"/>                
              <item name="stock-flow-flow-group-membership-datafeed"/>
              <item name="stock-flow-flow-multiplier-datafeed"/>               
              <item name="stock-flow-flow-spatial-multiplier-datafeed"/>
              <item name="stock-flow-stock-limit-datafeed"/>                                
            </group>
          </layout>
        </layouts>
      </transformer>
    </transformers>
    <updateProvider schemaVersion="11" schemaVersionTableName="SF_Version" className="SyncroSim.STSimStockFlow.SFUpdates" classAssembly="SyncroSim.STSimStockFlow"/>
    <console name="stockflow" displayName="Stocks and Flows Console" className="SyncroSim.STSimStockFlow.SFConsole" classAssembly="SyncroSim.STSimStockFlow"/>
  </module>
</configuration>