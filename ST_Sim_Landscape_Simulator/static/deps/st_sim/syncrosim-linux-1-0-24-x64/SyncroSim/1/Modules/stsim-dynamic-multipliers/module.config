<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <module name="stsim-dynamic-multipliers" displayName="Dynamic Multipliers" version="1.0024" configVersion="1.0000">
    <transformers>
      <transformer name="dhs-multipliers-core-transformer" displayName="Dynamic Multipliers" className="SyncroSim.STSimDynMult.DHSMTransformer" classAssembly="SyncroSim.STSimDynMult">
        <dataFeeds>
          <dataFeed name="dhs-multipliers-datafeed" displayName="DHS Multipliers" dataScope="Scenario" viewClassName="SyncroSim.STSimDynMult.DHSMultipliersDataFeedView" viewClassAssembly="SyncroSim.STSimDynMult">
            <dataSheets>
              <dataSheet name="DM_DHSMultipliers" displayName="DHS Multipliers" isSingleRow="True">
                <columns>
                  <column name="DHSMultipliersID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="Enabled" dataType="Boolean"/>
                  <column name="Frequency" dataType="Integer" validationType="WholeNumber" format="d"/>
                  <column name="StateAttributeTypeID" dataType="Integer" displayName="State Attribute Type" validationType="DataSheet" formula1="STSim_StateAttributeType"/>
                  <column name="Script" dataType="String"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
        </dataFeeds>
      </transformer>
      <transformer name="dynamic-multipliers-add-on-transformer" displayName="Dynamic Multipliers" isAddOn="True" extendsModel="stsim-model-transformer" extendsTransformer="stsim-core-transformer">
        <modelRunTransformers>
          <transformer name="dhs-multipliers-core-transformer"/>
        </modelRunTransformers>
        <layouts>
          <layout name="ScenarioDataFeeds" appendTo="Advanced">
            <item name="dhs-multipliers-datafeed"/>
          </layout>
        </layouts>
      </transformer>
    </transformers>
  </module>
</configuration>
