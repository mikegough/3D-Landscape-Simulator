<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <module name="syncrosim-statistics-module" displayName="SyncroSim Statistics" version="1.0000">
    <transformers>
      <transformer name="syncrosim-statistics-core-transformer" displayName="SyncroSim Statistics">
        <dataFeeds>
          <dataFeed name="syncrosim-statistics-distribution-type-datafeed" displayName="Distribution Types" dataScope="Project" viewClassName="SyncroSim.Statistics.DistributionTypeDataFeedView" viewClassAssembly="SyncroSim.Statistics">
            <dataSheets>
              <dataSheet name="Stats_DistributionType" displayName="Distribution Types" valueMember="DistributionTypeID" displayMember="Name" className="SyncroSim.Statistics.DistributionTypeDataSheet" classAssembly="SyncroSim.Statistics">
                <columns>
                  <column name="DistributionTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" displayName="Name" dataType="String"/>
                  <column name="Description" displayName="Description" dataType="String" isOptional="True"/>
                  <column name="IsInternal" dataType="Boolean" isVisible="False"/>
                </columns>
                <defaultRecords>
                  <record columns="Name|Description|IsInternal" values="Beta|Beta Distribution|-1"/>
                  <record columns="Name|Description|IsInternal" values="Normal|Normal Distribution|-1"/>
                  <record columns="Name|Description|IsInternal" values="Uniform|Uniform Distribution|-1"/>
                </defaultRecords>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="syncrosim-statistics-distribution-value-datafeed" displayName="Distributions" dataScope="Scenario" viewClassName="SyncroSim.Statistics.DistributionValueDataFeedView" viewClassAssembly="SyncroSim.Statistics">
            <dataSheets>
              <dataSheet name="Stats_DistributionValue" displayName="Distributions">
                <columns>
                  <column name="DistributionValueID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="DistributionTypeID" dataType="Integer" displayName="Distribution Type" validationType="DataSheet" formula1="Stats_DistributionType" allowDbNull="False"/>
                  <column name="Min" dataType="Double" validationType="Decimal" isOptional="True"/>
                  <column name="Max" dataType="Double" validationType="Decimal" allowDbNull="False"/>
                  <column name="RelativeFrequency" dataType="Double" displayName="Relative Frequency" validationType="Decimal" validationCondition="Greater" formula1="0.0" isOptional="True"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
        </dataFeeds>
      </transformer>
    </transformers>
    <updateProvider schemaVersion="1" schemaVersionTableName="Stats_Version" className="SyncroSim.Statistics.StatsUpdateProvider" classAssembly="SyncroSim.Statistics"/>
  </module>
</configuration>
