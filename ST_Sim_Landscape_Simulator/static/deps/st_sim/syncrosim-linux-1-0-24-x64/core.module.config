<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <module name="syncrosim-core-module" displayName="SyncroSim Core Services" version="1.0000">
    <transformers>
      <transformer name="syncrosim-core-transformer" displayName="SyncroSim Core">
        <dataFeeds>
          <dataFeed name="syncrosim-library-properties-datafeed" displayName="Summary" dataScope="Library" viewClassName="SyncroSim.Core.LibraryPropertiesDataFeedView" viewClassAssembly="SyncroSim.Core">
          </dataFeed>
          <dataFeed name="syncrosim-library-processing-datafeed" displayName="Processing" dataScope="Library" viewClassName="SyncroSim.Core.LibraryProcessingDataFeedView" viewClassAssembly="SyncroSim.Core">
            <dataSheets>
              <dataSheet name="SSim_Processing" displayName="Processing" isSingleRow="True" className="SyncroSim.Core.LibraryProcessingDataSheet" classAssembly="SyncroSim.Core">
                <columns>
                  <column name="ProcessingID" dataType="Integer" isPrimary="True"/>
                  <column name="EnableMultiProcessing" dataType="Boolean"/>
                  <column name="MaximumJobs" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0"/>
                  <column name="AutoDeleteTempFiles" dataType="Boolean"/>
                  <column name="EnableMultiScenario" dataType="Boolean"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>
          <dataFeed name="syncrosim-library-files-datafeed" displayName="Files" dataScope="Library" viewClassName="SyncroSim.Core.LibraryFilesDataFeedView" viewClassAssembly="SyncroSim.Core">
            <dataSheets>
              <dataSheet name="SSim_Files" displayName="Files" isSingleRow="True">
                <columns>
                  <column name="FilesID" dataType="Integer" isPrimary="True"/>
                  <column name="InputFolderName" dataType="String"/>
                  <column name="OutputFolderName" dataType="String"/>
                  <column name="TempFolderName" dataType="String"/>                  
                  <column name="ExcelTemplateFileName" dataType="String"/>
                </columns>
              </dataSheet>
            </dataSheets>
          </dataFeed>                   
        </dataFeeds>
      </transformer>
    </transformers>
    <dataProviders>
      <dataProvider name="excel-data-provider" displayName="Excel Data Provider" filter="Excel Files (*.xlsx)|*.xlsx" transferType="Scenario|DataSheet" className="SyncroSim.Core.ExcelDataProvider" classAssembly="SyncroSim.Core"/>
      <dataProvider name="csv-data-provider" displayName="CSV Data Provider" filter="CSV Files (*.csv)|*.csv" transferType="DataSheet" className="SyncroSim.Core.CSVDataProvider" classAssembly="SyncroSim.Core"/>
    </dataProviders>
    <updateProvider schemaVersion="21" schemaVersionTableName="SSim_Version" className="SyncroSim.Core.SystemUpdates" classAssembly="SyncroSim.Core"/>
    <console name="system" displayName="System Console" className="SyncroSim.Core.SystemConsole" classAssembly="SyncroSim.Core"/>
  </module>
</configuration>
